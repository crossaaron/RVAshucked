{{#if isAuthenticated}}
{{> header}}
<div class="container">
      <div class="jumbotron">
        <h1 class="display-3 jumboHeader">RVA Shucked</h1>
        <p class="lead jumboSubheader"></p>
      </div>
      <div class="row">
        <div style="margin-bottom:30px;" class="col-md-8">
          <iframe src="https://www.google.com/maps/d/embed?mid=1WzQLTuRn8EowgoG_Lgt3k-ZzWc0Bjbey" frameborder="0" width="100%" height="555px"></iframe>
        </div>

        <div class="col-md-4 oysterBio">
          <div class="card" style="width: 100%;">
            <img class="card-img-top" src="./../assets/img/oysters.jpg" alt="Card image cap">
            <div style="padding:10px" class="card-block">
              <h4 class="card-title">VA Oysters</h4>
              <p class="card-text">Richmond offers a wide variety of VA oysters. From sweet and salty to smooth and creamy, Virginia has one of the best selections of oysters.  Lucky for you, we've made it easy to start shucking in the Richmond area.  Please visit the availabitity page to find out where you're going for dinner.</p>
              <a href="https://www.virginia.org/oysters" target="_blank" class="btn btn-info learn">Learn more at VA Oysters</a>
            </div>
          </div>
        </div>
        <!-- SnapWidget -->
        <iframe src="https://snapwidget.com/embed/515161" class="snapwidget-widget" allowTransparency="true" frameborder="0" scrolling="no"
          style="border:none; overflow:hidden; width:925px; height:150px"></iframe>
      </div>
</div>
      <footer class="footer">
        <p class="foot">&copy; RVA Shucked 2018</p>
      </footer>

